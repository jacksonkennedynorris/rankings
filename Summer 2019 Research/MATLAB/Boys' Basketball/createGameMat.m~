function [GameMat,PD] = createGameMat(Games,Teams)
GameMat = zeros(length(Games),length(Teams));
PD = zeros(length(Games),1);
for elem = length(Games):-1:1
    win = Games(elem).winID;
    loss = Games(elem).loseID;
    pd = Games(elem).PD;
    if win < 1 || loss < 1
        GameMat(elem,:) = [];
        continue
    else
        GameMat(elem,win) = 1;
        GameMat(elem,loss) = -1;
        PD(elem) = pd;
    end
end
%% Remove teams no games
[row,col] = find(GameMat);
col
disp("HEY")