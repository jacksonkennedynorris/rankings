function writeFinalTable(Games,Teams,averageRating,dataFinal,year)


for elem = 1:length(Teams) 
    Teams(elem).Rating = averageRating(elem);
end

disp("DONE")

dataFinal.averageRating = averageRating;
averageRating = round(averageRating,2);

final = table(dataFinal.Name,dataFinal.Region,dataFinal.District,dataFinal.outOfStateRecord,dataFinal.inStateRecord,averageRating);
final.Properties.VariableNames = {'Name','Region','District','Record','In_State_Record','Rating'};
final = sortrows(final,6,'descend');
cd("../../../../github/rankings/BBasketball")
writetable(final,strcat(num2str(dataFinal.year),"table.csv"))
cd("../../../Google Drive (jackson.norris@centre.edu)/Summer 2019 Research/MATLAB/Boys' Basketball")
